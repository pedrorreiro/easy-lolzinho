{
  "name": "@pedrorreiro/lolzinho",
  "version": "0.0.1-dev.9",
  "description": "A simple library to make lolzinho",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node-dev 'src/index.ts'",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.5.0"
  }
}
