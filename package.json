{
  "name": "@pedrorreiro/lolzinho",
  "version": "0.0.1-dev.13",
  "description": "A simple library to make lolzinho",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "ts-node-dev 'src/index.ts'",
    "publish:dev": "./bin/publish-dev.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "package.json",
    "*.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.5.0"
  }
}
